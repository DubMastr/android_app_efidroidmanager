<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressCircle">
        <attr name="baseStrokeWidth" format="dimension"/>
        <attr name="progressStrokeWidth" format="dimension"/>
        <attr name="baseStrokeColor" format="color"/>
        <attr name="progressStrokeColor" format="color"/>
        <attr name="minValue" format="float"/>
        <attr name="maxValue" format="float"/>
        <attr name="currentValue" format="float"/>
        <attr name="startAngle" format="integer"/>
        <attr name="sweepAngle" format="integer"/>
        <attr name="fillColor" format="color"/>
        <attr name="contentText" format="string"/>
        <attr name="contentTextSize" format="dimension"/>
        <attr name="contentTextColor" format="color"/>
        <attr name="hideProgress" format="boolean"/>
    </declare-styleable>
</resources>